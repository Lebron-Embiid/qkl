<view class="myMember data-v-3c4ffcd9"><uni-nav-bar vue-id="1" title="我的会员" rightDot="{{dot}}" rightIcon="{{rightIcon}}" class="data-v-3c4ffcd9" bind:__l="__l"></uni-nav-bar><common-avatar vue-id="2" class="data-v-3c4ffcd9" bind:__l="__l"></common-avatar><view class="my_member_box data-v-3c4ffcd9"><view class="my_member_item first data-v-3c4ffcd9"><image src="/static/member_icon4.png" mode="widthFix" class="data-v-3c4ffcd9"></image><view class="right_area data-v-3c4ffcd9"><view class="member_txt data-v-3c4ffcd9">会员人数：</view><view class="member_money data-v-3c4ffcd9">{{person_num}}</view></view></view><view data-event-opts="{{[['tap',[['toDividend',['$event']]]]]}}" class="my_member_item data-v-3c4ffcd9" bindtap="__e"><image src="/static/member_icon3.png" mode="widthFix" class="data-v-3c4ffcd9"></image><view class="right_area data-v-3c4ffcd9"><view class="member_txt data-v-3c4ffcd9">团队分红：</view><view class="member_money data-v-3c4ffcd9">{{"$ "+dividend}}</view></view></view></view><view class="my_member_list data-v-3c4ffcd9"><block wx:for="{{memberList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="my_member_list_item data-v-3c4ffcd9"><view data-event-opts="{{[['tap',[['changeTitle',[index]]]]]}}" class="{{['my_member_nav data-v-3c4ffcd9',item.current==true?'active':'']}}" bindtap="__e"><text class="data-v-3c4ffcd9">{{item.title}}</text><image src="/static/down.svg" mode="widthFix" class="data-v-3c4ffcd9"></image></view><view class="{{['my_member_content data-v-3c4ffcd9',item.current==true?'active':'']}}"><uni-transition vue-id="{{'3-'+index}}" mode-class="{{['slide-top','fade']}}" show="{{item.current}}" class="data-v-3c4ffcd9" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{item.list}}" wx:for-item="list" wx:for-index="idx" wx:key="idx"><view class="my_member_content_item data-v-3c4ffcd9"><image src="{{list.avatar}}" mode="widthFix" class="data-v-3c4ffcd9"></image><view class="member_content_right data-v-3c4ffcd9"><view class="data-v-3c4ffcd9">{{"会员ID："+list.member_id}}</view><view class="data-v-3c4ffcd9">{{"昵称："+list.name}}</view><view class="data-v-3c4ffcd9">{{"上级："+list.superior}}</view><view class="data-v-3c4ffcd9">{{"投资总额："+list.price}}</view><view class="data-v-3c4ffcd9">{{"加入时间："+list.time}}</view><button type="primary" data-event-opts="{{[['tap',[['transferMoney',[index,idx]]]]]}}" bindtap="__e" class="data-v-3c4ffcd9">转款</button></view></view></block></uni-transition></view></view></block></view><uni-popup vue-id="4" type="center" data-ref="popup" class="data-v-3c4ffcd9 vue-ref" bind:__l="__l" vue-slots="{{['default']}}"><view class="popup_box data-v-3c4ffcd9"><view class="popup_content data-v-3c4ffcd9"><view class="popup_title data-v-3c4ffcd9">转款到对方APP钱包</view><view class="popup_info data-v-3c4ffcd9">{{"您将把资金转入到会员 "+name+" APP钱包"}}</view><input type="text" placeholder="请输入转款金额" data-event-opts="{{[['input',[['__set_model',['','price','$event',[]]]]]]}}" value="{{price}}" bindinput="__e" class="data-v-3c4ffcd9"/><input type="password" placeholder="请输入支付密码" data-event-opts="{{[['input',[['__set_model',['','pay_pwd','$event',[]]]]]]}}" value="{{pay_pwd}}" bindinput="__e" class="data-v-3c4ffcd9"/></view><view class="popup_btn data-v-3c4ffcd9"><view data-event-opts="{{[['tap',[['cancelPopup',['$event']]]]]}}" bindtap="__e" class="data-v-3c4ffcd9">取消</view><view data-event-opts="{{[['tap',[['okPopup',['$event']]]]]}}" class="ok data-v-3c4ffcd9" bindtap="__e">确定</view></view></view></uni-popup></view>