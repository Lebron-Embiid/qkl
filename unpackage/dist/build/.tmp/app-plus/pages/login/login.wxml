<view class="login data-v-e9ea1aa0"><view class="login_top data-v-e9ea1aa0"><image src="{{logoSrc}}" mode="widthFix" class="data-v-e9ea1aa0"></image><text class="data-v-e9ea1aa0">{{app_name}}</text></view><view class="login_box data-v-e9ea1aa0"><form data-event-opts="{{[['submit',[['loginSubmit',['$event']]]]]}}" bindsubmit="__e" class="data-v-e9ea1aa0"><view class="form_item data-v-e9ea1aa0"><view class="icon no data-v-e9ea1aa0"><image src="/static/phone.svg" mode="widthFix" class="data-v-e9ea1aa0"></image></view><view class="right_box all data-v-e9ea1aa0"><view class="ipt_box data-v-e9ea1aa0"><input type="text" placeholder="手机号码" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e" class="data-v-e9ea1aa0"/><image class="{{['data-v-e9ea1aa0',phone!=''?'active':'']}}" src="/static/clear.svg" mode="widthFix" data-event-opts="{{[['tap',[['clearPhone',['$event']]]]]}}" bindtap="__e"></image></view></view></view><view class="form_item data-v-e9ea1aa0"><view class="icon data-v-e9ea1aa0"><image src="/static/pwd.svg" mode="widthFix" class="data-v-e9ea1aa0"></image></view><view class="right_box data-v-e9ea1aa0"><view class="ipt_box data-v-e9ea1aa0"><block wx:if="{{input_type==0}}"><input type="password" placeholder="登录密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e" class="data-v-e9ea1aa0"/></block><block wx:else><input type="text" placeholder="登录密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e" class="data-v-e9ea1aa0"/></block><image class="{{['data-v-e9ea1aa0',password!=''?'active':'']}}" src="/static/clear.svg" mode="widthFix" data-event-opts="{{[['tap',[['clearPwd',['$event']]]]]}}" bindtap="__e"></image></view><switchc class="switch_btn data-v-e9ea1aa0" vue-id="1" text="可见|***" sid="{{0}}" data-event-opts="{{[['^change',[['switchchange']]]]}}" bind:change="__e" bind:__l="