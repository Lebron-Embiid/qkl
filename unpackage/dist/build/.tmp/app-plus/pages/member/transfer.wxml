<view class="transfer data-v-447ec2ab"><uni-nav-bar vue-id="1" left-icon="back" leftText="返回" title="转款" rightDot="{{dot}}" rightIcon="{{rightIcon}}" class="data-v-447ec2ab" bind:__l="__l"></uni-nav-bar><common-avatar vue-id="2" class="data-v-447ec2ab" bind:__l="__l"></common-avatar><view class="member_info_box data-v-447ec2ab"><view class="member_list data-v-447ec2ab"><block wx:for="{{memberList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toListLink',[index]]]]]}}" class="member_item data-v-447ec2ab" bindtap="__e"><image src="{{item.icon}}" mode="widthFix" class="data-v-447ec2ab"></image><view class="right_area data-v-447ec2ab"><view class="member_txt data-v-447ec2ab">{{item.title+"："}}</view><view class="member_money data-v-447ec2ab">{{"$ "+item.value}}</view></view></view></block></view></view><view class="increase_apply data-v-447ec2ab"><text class="data-v-447ec2ab">转款操作</text><image src="/static/scan.svg" mode="widthFix" data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" bindtap="__e" class="data-v-447ec2ab"></image></view><block wx:if="{{is_apply==1}}"><view class="increase_before data-v-447ec2ab"><image src="/static/success.svg" mode="widthFix" class="data-v-447ec2ab"></image><view class="ib_title data-v-447ec2ab">操作成功</view><view class="ib_info data-v-447ec2ab">您已成功转款<text class="data-v-447ec2ab">{{send_price}}</text>{{'到'+name+"钱包中"}}</view><button class="black data-v-447ec2ab" type="primary" data-event-opts="{{[['tap',[['toHistory',['$event']]]]]}}" bindtap="__e">历史转款</button></view></block><block wx:if="{{is_apply==0}}"><view class="increase_after data-v-447ec2ab"><view class="form_item data-v-447ec2ab"><view class="icon no data-v-447ec2ab"><image src="/static/account.png" mode="widthFix" class="data-v-447ec2ab"></image></view><view class="right_box all data-v-447ec2ab"><view class="ipt_box data-v-447ec2ab"><input type="text" placeholder="请输入会员ID或钱包地址" data-event-opts="{{[['input',[['__set_model',['','id_link','$event',[]]]]]]}}" value="{{id_link}}" bindinput="__e" class="data-v-447ec2ab"/></view></view></view><view class="form_item data-v-447ec2ab"><view class="icon no data-v-447ec2ab"><image src="/static/add.png" mode="widthFix" class="data-v-447ec2ab"></image></view><view class="right_box all data-v-447ec2ab"><view class="ipt_box data-v-447ec2ab"><input type="text" placeholder="请输入转款金额" data-event-opts="{{[['input',[['__set_model',['','price','$event',[]]]]]]}}" value="{{price}}" bindinput="__e" class="data-v-447ec2ab"/></view></view></view><view class="form_item data-v-447ec2ab"><view class="icon data-v-447ec2ab"><image src="/static/pwd.svg" mode="widthFix" class="data-v-447ec2ab"></image></view><view class="right_box data-v-447ec2ab"><view class="ipt_box data-v-447ec2ab"><block wx:if="{{input_type==0}}"><input type="password" placeholder="请输入支付密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e" class="data-v-447ec2ab"/></block><block wx:else><input type="text" placeholder="请输入支付密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e" class="data-v-447ec2ab"/></block></view><switchc class="switch_btn data-v-447ec2ab" vue-id="3" text="可见|***" sid="{{0}}" data-event-opts="{{[['^change',[['switchchange']]]]}}" bind:change="__e" bind:__l="__l"></switchc></view></view><button class="submit_btn data-v-447ec2ab" form-type="submit" data-event-opts="{{[['tap',[['applyConfirm',['$event']]]]]}}" bindtap="__e">提交确认</button><button data-event-opts="{{[['tap',[['toHistory',['$event']]]]]}}" class="submit_btn history_btn data-v-447ec2ab" bindtap="__e">历史转款</button></view></block></view>